
@{
    ViewBag.Title = "Primeiro Acesso Voluntário";
}

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <title>_Layout</title>
</head>

<h2>Primeiro Acesso Voluntário</h2>

<form action="/Login/GravarPrimeiroAcessoVoluntario" method="post">

    <label for="nome">Nome</label>

    <input type="text" id="nome" name="voluntario.nome" class="form-control" />

    <input type="text" id="cidade" name="voluntario.cidade" class="form-control" />

    <input type="text" id="usuario" name="voluntario.usuario" class="form-control" />

    <input type="text" id="senha" name="voluntario.senha" class="form-control" />

    <input type="submit" value="Gravar" class="form-control" />

</form>

</body>
</html>
